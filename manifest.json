{
  "manifest_version": 3,
  "name": "YouTube Timestamp Bookmark",
  "version": "1.0",
  "description": "Bookmark specific timestamps in YouTube videos.",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "bookmark-icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "bookmark-icon.png"
  }
}
